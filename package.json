  "engines": {
    "node": ">=18"
  },
  "name": "boilerplate",
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "expo-router": "^4.0.17"
  }
}
